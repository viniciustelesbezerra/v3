<h1>Arquivos</h1>

<table class="table table-hover">
  <tr>
    <th>Tipo</th>
    <th>Em uso</th>
    <th>Carregado evora</th>
    <th>Data carregado evora</th>
    <th>User</th>
    <th>File</th>
    <th></th>
    <th></th>
  </tr>

<% @arquivos.each do |arquivo| %>
  <tr>
    <td><%= arquivo.tipo %></td>
    <td><%= arquivo.em_uso %></td>
    <td><%= arquivo.carregado_evora %></td>
    <td><%= arquivo.data_carregado_evora %></td>
    <td><%= arquivo.user.name %></td>
    <td><%= arquivo.image_url.to_s %></td>
    <td><%= link_to 'Show', arquivo %></td>
    <td><%= link_to 'Edit', edit_arquivo_path(arquivo) %></td>
    <td><%= link_to 'Destroy', arquivo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Arquivo', new_arquivo_path %>
